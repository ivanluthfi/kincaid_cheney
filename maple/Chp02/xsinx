# Evaluate x - sin x at different points
#
y := sum( (-1)^(k-1)*x^(2*k+1)/(2*k+1)!, k=1..10 );
Digits := 100;
x := 0.4;
z := evalf(y);
t := x - sin(x);
d := z - t;
