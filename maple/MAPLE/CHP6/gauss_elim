# Using Maple Linear Algebra package to solve
# the following linear system:

 with(linalg):
 A := array([[ 3, -13,  9,   3, -19],
             [-6,   4,  1, -18, -34],
             [ 6,  -2,  2,   4,  16],
             [12,  -8,  6,  10,  26]]);
 L := gausselim(A);
 x := backsub(L);