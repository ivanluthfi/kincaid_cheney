# Solving a linear system
#
with(linalg):
A := array([[ 6,  -2, 2,   4],
	    [12,  -8, 6,  10],
	    [ 3, -13, 9,   3],
	    [-6,   4, 1, -18]]);	
b := array( [16, 26, -19, -34]);
linsolve(A, b);